{

    "name": "Authnull",
    "description": "Authnull-extension",
    "version": "1.0",
    "manifest_version": 3,
    "background" : {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "images/bijay.jpeg",
      "32": "images/share.svg"
   },
    
    "permissions": ["activeTab",
                     "notifications", "alarms", "background"],
    
    "action": {
      "default_popup": "index.html"
    }
    
    
    
}